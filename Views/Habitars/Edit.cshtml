@model inviWebApp.Models.Habitar

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Habitar</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Curp" class="control-label"></label>
                <select asp-for="Curp" class="form-control" asp-items="ViewBag.Curp"></select>
                <span asp-validation-for="Curp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Vivienda" class="control-label"></label>
                <select asp-for="Vivienda" class="form-control" asp-items="ViewBag.Vivienda"></select>
                <span asp-validation-for="Vivienda" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
